#include <stdio.h>

int main() {
    int fechas[9][3];
    
    for(int i = 1;i<=9;i++){
        printf("Ingrese Fecha De Nacimiento");
    for(int j = 1;j<=3;j++);
       printf("\ningrese fecha de nacimiento");
       printf("\ningrese fecha de nacimiento : ");
     scanf("%d", &fechas[i-1][0]);
     printf("\ningrese el mes de nacimiento : ");
     scanf("%d", &fechas[i-1][1]);
     printf("\ningrese el anio de nacimiento : ");
     scanf("%d", &fechas[i-1][2]);
    }
    printf("PERSONA  DIA  MES  ANIO");
    for(int i = 1;i<=9;i++){
        printf("  %d    %d    %d    %d\n",i, fechas[i-1][0],fechas[i-1][1,fechas[i-1][2]);
            
    }
    
return 0;
} 
